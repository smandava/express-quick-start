{
  "name": "express-quick-start",
  "version": "1.0.0",
  "description": "Using latest JavaScript features on the server",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "^10.x",
    "yarn": "^1.9"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"eslint '**/*js' && node -r dotenv/config -r babel-register\"",
    "predocker:start": "yarn build",
    "docker:start": "docker-compose kill && docker-compose rm -f && docker-compose build && docker-compose run --rm --service-ports app",
    "clean": "rimraf dist",
    "build": "yarn run clean && mkdir dist && babel src -s -D -d dist",
    "lint": "esw -w src"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "eslint-plugin-prettier": "^3.0.0",
    "express": "^4.17.3",
    "morgan": "^1.9.0",
    "pug": "^2.0.0-beta11",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-register": "^6.26.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-watch": "^6.0.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.14.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "eslintIgnore": [
    "dist/**"
  ],
  "prettier": {
    "trailingComma": "all"
  }
}
